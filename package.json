{
  "name": "stradl",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"tsx watch server/index.ts\" \"vite\"",
    "build": "vite build && tsc -p tsconfig.server.json",
    "start": "node server/dist/index.js",
    "preview": "vite preview",
    "install-service": "node scripts/install-service.js",
    "uninstall-service": "node scripts/uninstall-service.js",
    "test": "vitest run --config vitest.config.server.ts",
    "test:watch": "vitest --config vitest.config.server.ts"
  },
  "dependencies": {
    "express": "^4.18.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "concurrently": "^8.2.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vite": "^5.4.0",
    "vite-plugin-pwa": "^0.19.0",
    "vitest": "^4.0.18"
  }
}
